<view class="bg-white" style="min-height:100vh;"><cu-custom vue-id="1" bgColor="bg-black" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">修改信息</view></cu-custom><view class="cu-form-group" style="border:0;"><view class="title">店名</view><input placeholder="店名" name="input" data-event-opts="{{[['input',[['__set_model',['$0','title','$event',[]],['info']]]]]}}" value="{{info.title}}" bindinput="__e"/><text class="cuIcon-newsfill text-main"></text></view><view data-event-opts="{{[['tap',[['ChangeLocation',['$event']]]]]}}" class="cu-form-group" style="border:0;" bindtap="__e"><view class="title">地址</view><text>{{info.address}}</text><text class="cuIcon-locationfill text-main"></text></view><view class="cu-form-group" style="border:0;"><view class="title">电话号码</view><input placeholder="身高" type="number" name="input" data-event-opts="{{[['input',[['__set_model',['$0',0,'$event',[]],['info.tel']]]]]}}" value="{{info.tel[0]}}" bindinput="__e"/><text class="cuIcon-phone text-main"></text></view><view class="cu-form-group" style="border:0;"><view class="title">备用号码</view><input placeholder="身高" type="number" name="input" data-event-opts="{{[['input',[['__set_model',['$0',1,'$event',[]],['info.tel']]]]]}}" value="{{info.tel[1]}}" bindinput="__e"/><text class="cuIcon-phone text-main"></text></view><view class="cu-bar tabbar bg-white  foot" style="justify-content:center;"><button data-event-opts="{{[['tap',[['Change',['$event']]]]]}}" class="cu-btn lg bg-main2 round margin-right" style="width:40%;" bindtap="__e"><text class="cuIcon-edit margin-right-sm text-red"></text>确定修改</button></view></view>